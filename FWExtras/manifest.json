{
    "manifest_version": 2,

    "name": "FWExtras",
    "version": "1.1",

    "description": "Enhances your Fluff World experience.",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "background": {
        "persistent": true,
        "scripts": [
            "modules/interceptor.js"
        ]
    },
    "permissions": [
        "cookies",
        "storage",
        "*://108.197.28.233/*",
        "webRequest",
        "webRequestBlocking"
    ],

    "content_scripts" : [{
        "matches": [
            "*://108.197.28.233/*"
        ],
        "css": [
            "css/fw-extra.css"
        ],
        "js" : [
            "lib/jquery-2.1.4.min.js",
            "lib/jquery-plugins.js",
            "lib/materialize.js",
            "modules/util.js",
            "modules/fluffcode.js",
            "modules/chat.js",
            "modules/visual.js",
            "modules/nicknames.js",
            "modules/settings.js"
        ]
        }]
    }
