{
    "manifest_version": 2,

    "name": "FWExtras",
    "version": "1.1",

    "description": "A Chrome extension like Reddit Enhancement Suite that improves FW using only JavaScript injections",
    "icons": {
        "16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "permissions": [
        "cookies",
        "storage"
    ],

    "content_scripts" : [{
        "matches": [
            "*://108.197.28.233/*"
        ],
        "css": [
            "css/fw-extra.css"
        ],
        "js" : [
            "lib/jquery-2.1.4.min.js",
            "lib/jquery-plugins.js",
            "modules/fluffcode.js",
            "modules/chat.js",
            "modules/visual.js",
            "modules/aliases.js",
            "modules/settings.js"
        ]
    }]
}
